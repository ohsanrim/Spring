<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">

<!-- sample01 -->

<bean id="messageBeanImpl" class="sample01.MessageBeanImpl">
	<constructor-arg>
		<value>딸기</value>
	</constructor-arg>
	<property name="cost">
		<value>3500</value>
	</property>
	<property name="qty" value="2"></property>
</bean>
<bean id="calcAdd" class="sample02.CalcAdd">
	<constructor-arg>
		<value>25</value>
	</constructor-arg>
		<constructor-arg>
		<value>20</value>
	</constructor-arg>
</bean>
<bean id="calcMul" class="sample02.CalcMul">
	<property name="num">
		<value>25</value>
	</property>
	<property name="num2">
		<value>20</value>
	</property>
</bean>

<!--sample03 -->

<bean id="SungJukImpl" class="sample03.SungJukImpl">
	<constructor-arg ref="sungJukDTO"></constructor-arg>
</bean>
	
<bean id="sungJukDTO" class="sample03.SungJukDTO">
	<property name="name">
		<value>홍길동</value>
	</property>
	<property name="kor">
		<value>20</value>
	</property>
	<property name="eng">
		<value>76</value>
	</property>
		<property name="math">
		<value>87</value>
	</property>
</bean>

<!-- sample04 -->

<bean id="messageBeanImpl2" class="sample04.MessageBeanImpl" >
	<constructor-arg>
		<value>홍길동</value>
	</constructor-arg>
	<property name="phone">
		<value>010-123-1234</value>
	</property>
	<property name="outputter" ref="fileOutputter"></property>
</bean>

<bean id="fileOutputter" class="sample04.FileOutputter" >
	<property name="filePath" value="D:/"></property>
	<property name="fileName" value="result.txt"></property>
</bean>


<!-- sample05 -->

<bean id="list" class="java.util.ArrayList" >
</bean>

<bean id="sungJukDTO2" class="sample05.SungJukDTO2" scope="prototype" >
</bean>

<bean id="sungJukInput" class="sample05.SungJukInput" scope="prototype">
	<property name="list" ref="list"></property>
	<property name="sungJukDTO2" ref="sungJukDTO2"></property>
</bean>

<bean id="sungJukOutput" class="sample05.SungJukOutput">
<property name="list" ref="list"></property>
</bean>

<bean id="sungJukModify" class="sample05.SungJukModify">
<property name="list" ref="list"></property>
</bean>

<bean id="sungJukDelete" class="sample05.SungJukDelete">
<property name="list" ref="list"></property>
</bean>

</beans>
